{
  "rules": {
    ".read": false,
    ".write": false,

    "presence": {
      "$projectId": {
        ".read": "auth != null",
        "$userId": {
          ".read": "auth != null",
          ".write": "auth != null && auth.uid == $userId"
        }
      }
    },

    "cursors": {
      "$projectId": {
        ".read": "auth != null",
        "$userId": {
          ".read": "auth != null",
          ".write": "auth != null && auth.uid == $userId"
        }
      }
    }
  }
}
